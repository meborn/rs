!!!
%html
	%head
		%title RynoStand
		%meta{:content => "width=device-width, initial-scale=1", :name => "viewport"}
		= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true
		= javascript_include_tag 'application', 'data-turbolinks-track' => true
		= csrf_meta_tags
	%body
		-if flash[:notice]
			%p{class:"notice"}
				=notice
		-if flash[:error]
			%p{class:"alert"}
				=alert
		= render 'shared/nav'
		= yield
